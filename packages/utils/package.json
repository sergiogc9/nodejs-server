{
	"name": "@sergiogc9/nodejs-utils",
	"version": "0.1.0",
	"description": "NodeJS TypeScript utils",
	"author": "Sergio GÃ³mez Cosgaya (sergiogcosgaya@gmail.com)",
	"repository": "git://github.com/sergiogc9/nodejs-server.git",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": "./dist/cjs/index.js",
		"./Api": "./dist/cjs/Api.js",
		"./Cache": "./dist/cjs/Cache.js",
		"./Log": "./dist/cjs/Log.js",
		"./Model": "./dist/cjs/Model.js",
		"./Pushover": "./dist/cjs/Pushover.js"
	},
	"typesVersions": {
		"*": {
			"Api": [
				"dist/types/Api"
			],
			"Cache": [
				"dist/types/Cache"
			],
			"Log": [
				"dist/types/Log"
			],
			"Model": [
				"dist/types/Model"
			],
			"Pushover": [
				"dist/types/Pushover"
			]
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepare": "rollup -c ./rollup.config.js",
		"watch": "rollup -w -c ./rollup.config.js"
	},
	"dependencies": {
		"express": "4.17.1",
		"lodash": "4.17.21",
		"luxon": "1.25.0",
		"memory-cache": "0.2.0",
		"mongoose": "5.11.13",
		"pushover-notifications": "1.2.2"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "3.1.1",
		"@rollup/plugin-typescript": "8.1.0",
		"@types/lodash": "4.14.166",
		"@types/luxon": "1.25.0",
		"@types/memory-cache": "0.2.1",
		"rollup": "2.35.1",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-dts": "2.0.1",
		"rollup-plugin-terser": "7.0.2"
	},
	"engines": {
		"node": "14.15.x",
		"npm": ">=6.14.0"
	}
}
